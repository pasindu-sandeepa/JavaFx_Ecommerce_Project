<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.LoginForm">
   <children>
      <Label layoutX="286.0" layoutY="46.0" text="Login" />
      <Label layoutX="156.0" layoutY="161.0" prefHeight="18.0" prefWidth="54.0" text="Password" />
      <Label layoutX="156.0" layoutY="102.0" text="Username" />
      <TextField fx:id="txtUsernameLoginForm" layoutX="257.0" layoutY="97.0" onAction="#txtUsernameLoginFormOnAction" prefHeight="26.0" prefWidth="209.0" />
      <Hyperlink fx:id="hykLoginForm" layoutX="268.0" layoutY="266.0" onAction="#hykLoginFormOnAction" prefHeight="24.0" prefWidth="188.0" text="Don't have an account? Register" />
      <Button fx:id="btnLoginLoginForm" layoutX="313.0" layoutY="232.0" mnemonicParsing="false" onAction="#btnLoginLoginFormOnAction" prefHeight="26.0" prefWidth="97.0" text="Login" />
      <PasswordField fx:id="txtPasswordLoginForm" layoutX="257.0" layoutY="157.0" onAction="#txtPasswordLoginFormOnAction" prefHeight="26.0" prefWidth="210.0" />
      <Hyperlink fx:id="hykForgetPassword" layoutX="368.0" layoutY="188.0" onAction="#hykForgetPasswordOnAction" prefHeight="24.0" prefWidth="98.0" text="Forget Password" />
